CREATE TABLE users (
    id INTEGER PRIMARY KEY,
    name TEXT,
    email TEXT,
    age INTEGER
);
 INSERT INTO users (id, name, email, age) VALUES
(1, 'Amit', 'amit@gmail.com', 20),
(2, 'Riya', 'riya@yahoo.com', 17),
(3, 'Kunal', 'kunal@gmail.com', 25);
 SELECT * FROM users;
 SELECT * FROM users
WHERE age >= 18;
 UPDATE users
SET age = 22
WHERE id = 1;
DELETE FROM users
WHERE id = 2;
 SELECT * FROM users
ORDER BY age ASC;
 SELECT * FROM users
ORDER BY age DESC;
 SELECT * FROM users
WHERE email LIKE '%@gmail.com';
